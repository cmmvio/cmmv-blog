{
    "name": "@cmmv/blog",
    "version": "0.0.1",
    "description": "Blog plugin for CMMV",
    "keywords": [
        "cmmv",
        "plugin",
        "blog",
        "cms",
        "content",
        "management",
        "system"
    ],
    "license": "MIT",
    "author": "Andre Ferreira",
    "packageManager": "pnpm@10.6.5",
    "exports": {
        "./admin": "./dist/admin/index.mjs",
        "./api": "./dist/api/index.mjs",
        "./client": "./dist/client/index.mjs",
        "./contracts": "./dist/contracts/index.mjs"
    },
    "typesVersions": {
        "*": {
            "client": ["dist/client/index.d.ts"],
            "admin": ["dist/admin/index.d.ts"],
            "api": ["dist/api/index.d.ts"],
            "contracts": ["dist/contracts/index.d.ts"]
        }
    },
    "files": [
        "dist",
        "package.json",
        "README.md"
    ],
    "engines": {
        "node": ">= 20.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "cli-cursor": "^5.0.0",
        "restore-cursor": "^5.1.0",
        "signal-exit": "^4.1.0"
    },
    "scripts": {
        "dev": "turbo run dev",
        "build": "turbo run build --filter=@cmmv/blog",
        "build:api": "cd apps/api && pnpm build",
        "build:admin": "cd apps/admin && pnpm build",
        "build:web": "cd apps/web && pnpm build",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "release": "pnpm build && pnpm publish --tag latest -r --no-git-checks",
        "info": "turbo run info",
        "start:api": "cd apps/api && pnpm start",
        "start:admin": "cd apps/admin && pnpm start",
        "start:web": "cd apps/web && pnpm start"
    },
    "devDependencies": {
        "eslint": "^9.24.0",
        "prettier": "^3.5.3",
        "turbo": "^2.5.0"
    }
}